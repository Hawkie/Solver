<UserControl x:Class="SolverModules.Sudoku.SudokuView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="400" Width="500" >
    <StackPanel>
        <Grid x:Name="grid1"></Grid>
        <WrapPanel>
            <Button Height="23" Name="loadButton" Width="75" Click="loadButton_Click">Load</Button>
            <Button x:Name="solveButton" Width="75"   Click="Button_Click">Solve</Button>
            <Button x:Name="resetButton" Width="75" Click="resetButton_Click">Reset</Button>
        </WrapPanel>
    </StackPanel>
</UserControl>
